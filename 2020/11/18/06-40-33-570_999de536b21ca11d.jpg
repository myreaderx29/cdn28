<script type="text/javascript">
  $(function() {
    const EVENT_NAME = 'login_page_error_render';

    const error = '';
    const alert = '';
    const notice = '';
    const info = '';

    if (error) {
      BigBrain('track', EVENT_NAME, { kind: 'error', info1: error });
    }

    if (alert) {
      BigBrain('track', EVENT_NAME, { kind: 'alert', info1: alert });
    }

    if (notice) {
      BigBrain('track', EVENT_NAME, { kind: 'notice', info1: notice });
    }

    if (info) {
      BigBrain('track', EVENT_NAME, { kind: 'info', info1: info });
    }
  });
</script>
