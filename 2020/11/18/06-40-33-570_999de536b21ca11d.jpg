<script type="text/javascript">
  $(function() {
    var isInIframe = window.location != window.parent.location;
    BigBrain("track", "login_page_load", { placement: "sso", kind: 'okta' });

    $('#google_apps_login .signup_button').each(function() {
      if(isInIframe) {
        Ladda.bind($(this)[0], {timeout: 0});
      } else {
        Ladda.bind($(this)[0]);
      }
    });

    $("#google_apps_login .email_password_button").on('click', function(){
      BigBrain("track", "login_page_click", { placement: "sso", kind: 'login_with_email_password', info1:'okta' });
