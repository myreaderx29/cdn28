  <script type="text/javascript">

function isInIframe(){
  try {
    return window.location != window.parent.location;    
  } catch (error) {
    console.error(error);
    return false;  
  }

}

function isInPopUp(){
  try {
    return window.opener && window.opener !== window;    
  } catch (error) {
    console.error(error);
    return false;
  }
}

function isInMicrosoftTeamsApp(){
  try {
    var user_agent = navigator.userAgent;
    return user_agent && user_agent.includes(" Teams/") && user_agent.includes(" Electron/");
  } catch(error){
    console.error(error);
    return false;
  }
}


window.LoginHelpers = {

  show_error: function (msg) {
    var content_div = $('.content')[0];

    // remove old error messages
    $('.alert-danger').remove();

    var error_div = $("<div class='alert alert-danger'>"+msg+"</div>");
    $(content_div).prepend($(error_div));
  },

  is_in_iframe: function() {
    return isInIframe();
  },

  is_in_pop_up: function() {
    return isInPopUp();
  },

  is_in_microsoft_teams_app: function() {
    return isInMicrosoftTeamsApp();
  },

  open_in_new_tab_if_needed: function(url) {
    const isIframe = isInIframe();
    const isMicrosoftTeamsApp = isInMicrosoftTeamsApp();
    if(!isMicrosoftTeamsApp && isIframe){
      // the page is in an iframe, so we want to open the login in a new tab/window
      window.open(url,'_blank');
    } else {
      window.location = url;
    }
  }
}
</script>
