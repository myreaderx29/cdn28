    })

    $(".login_to_another_account").on('click', function(){
      BigBrain("track", "login_page_click", { placement: "sso", kind: "login_to_another_account", info1:'okta' });
    });

    $("#google_apps_login .signup_button").on('click', function(){
      var url = "https://lucasfilm.monday.com/admin/sso_connectors/login";
      if(isInIframe){
        window.open(url, '_blank');
      } else {
        window.location = url;
      }
    });
  });
</script>

